{
  "name": "@builder.io/html-to-figma-chrome-extension",
  "version": "0.0.1",
  "description": "Chrome extension to grab current page HTML and import it to Figma",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.prod.js",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.dev.js --watch",
    "watch": "NODE_OPTIONS=--openssl-legacy-provider webpack -w --config webpack.dev.js",
    "lint": "eslint 'src/**/*.{ts, tsx}'"
  },
  "typings": "dist/main.d.ts",
  "author": "Steve Sewell",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra-promise": "^1.0.8",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "css-loader": "^7.1.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.37.2",
    "fs-extra-promise": "^1.0.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@builder.io/html-to-figma": "^0.0.5-3",
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "@types/chrome": "^0.0.287",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass": "^1.54.7"
  }
}
